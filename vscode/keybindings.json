[
  {
    "key": "alt+,",
    "command": "extension.multiCommand.execute",
    "args": {
      "sequence": [
        "editor.action.addSelectionToNextFindMatch",
        "workbench.action.findInFiles"
      ]
    },
    "when": "editorHasDefinitionProvider && editorTextFocus && !isInEmbeddedEditor"
  },
  {
    "key": "alt+.",
    "command": "editor.action.revealDefinition",
    "when": "editorHasDefinitionProvider && editorTextFocus && !isInEmbeddedEditor"
  },
  {
    "key": "alt+;",
    "command": "editor.action.commentLine",
    "when": "editorTextFocus && !config.emacs-mcx.useMetaPrefixMacCmd && !editorReadonly"
  },
  {
    "key": "alt+k",
    "command": "emacs-mcx.paredit.killSexp",
    "when": "editorTextFocus"
  },
  {
    "key": "alt+tab",
    "command": "editor.foldRecursively",
    "when": "editorTextFocus && foldingEnabled"
  },
  {
    "key": "cmd+/",
    "command": "-editor.action.commentLine",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "cmd+/",
    "command": "extension.multiCommand.execute",
    "args": {
      "sequence": [
        "workbench.action.focusFirstEditorGroup",
        "workbench.action.togglePanel"
      ]
    },
    "when": "panelFocus"
  },
  {
    "key": "cmd+/",
    "command": "extension.multiCommand.execute",
    "args": {
      "sequence": ["workbench.action.focusPanel"]
    },
    "when": "!panelFocus"
  },
  {
    "key": "cmd+;",
    "command": "editor.action.blockComment",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "cmd+\\",
    "command": "editor.toggleFold",
    "when": "editorTextFocus && foldingEnabled"
  },
  {
    "key": "cmd+enter",
    "command": "-editor.action.insertLineAfter",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "cmd+enter",
    "command": "extension.multiCommand.execute",
    "args": {
      "sequence": [
        "workbench.action.closeSidebar",
        "workbench.action.closePanel",
        "workbench.action.closeEditorsInOtherGroups"
      ]
    },
    "when": "!panelFocus"
  },
  {
    "key": "cmd+enter",
    "command": "workbench.action.toggleMaximizedPanel",
    "when": "panelFocus"
  },
  {
    "key": "cmd+k cmd+l",
    "command": "-editor.toggleFold",
    "when": "editorTextFocus && foldingEnabled"
  },
  {
    "key": "cmd+left",
    "command": "workbench.action.navigateLeft"
  },
  {
    "key": "cmd+down",
    "command": "workbench.action.navigateDown"
  },
  {
    "key": "cmd+up",
    "command": "workbench.action.navigateUp"
  },
  {
    "key": "cmd+m",
    "command": "magit.status"
  },
  {
    "key": "cmd+n",
    "command": "workbench.action.quickOpen"
  },
  {
    "key": "cmd+o",
    "command": "newFile.createNewFile"
  },
  {
    "key": "cmd+q",
    "command": "workbench.action.quickOpenPreviousRecentlyUsedEditor",
    "when": "!inEditorsPicker && !inQuickOpen"
  },
  {
    "key": "cmd+q",
    "command": "workbench.action.quickOpenNavigateNextInEditorPicker",
    "when": "inEditorsPicker && inQuickOpen"
  },
  {
    "key": "cmd+right",
    "command": "workbench.action.navigateRight"
  },
  {
    "key": "cmd+s",
    "command": "extension.multiCommand.execute",
    "args": {
      "sequence": [
        "hideSuggestWidget",
        "removeSecondaryCursors",
        "workbench.action.files.save"
      ]
    },
    "when": "editorFocus"
  },
  {
    "key": "ctrl+down",
    "command": "goToNextReference",
    "when": "inReferenceSearchEditor || referenceSearchVisible"
  },
  {
    "key": "ctrl+down",
    "command": "references-view.next",
    "when": "reference-list.hasResult && references-view.canNavigate"
  },
  {
    "key": "ctrl+down",
    "command": "search.action.focusNextSearchResult",
    "when": "hasSearchResult || inSearchEditor"
  },
  {
    "key": "ctrl+end",
    "command": "terminal.focus",
    "when": "inDebugRepl"
  },
  {
    "key": "ctrl+end",
    "command": "workbench.panel.output.focus",
    "when": "problemFocus || problemsView || problemsFilterFocus"
  },
  {
    "key": "ctrl+end",
    "command": "workbench.panel.repl.view.focus",
    "when": "inOutput"
  },
  {
    "key": "ctrl+enter",
    "command": "workbench.action.toggleMaximizedPanel"
  },
  {
    "key": "ctrl+home",
    "command": "workbench.action.problems.focus",
    "when": "inOutput"
  },
  {
    "key": "ctrl+home",
    "command": "workbench.panel.output.focus",
    "when": "inDebugRepl"
  },
  {
    "key": "ctrl+home",
    "command": "workbench.panel.repl.view.focus",
    "when": "terminalFocus"
  },
  {
    "key": "ctrl+tab",
    "command": "editor.unfoldRecursively",
    "when": "editorTextFocus && foldingEnabled"
  },
  {
    "key": "ctrl+up",
    "command": "goToPreviousReference",
    "when": "inReferenceSearchEditor || referenceSearchVisible"
  },
  {
    "key": "ctrl+up",
    "command": "references-view.prev",
    "when": "reference-list.hasResult && references-view.canNavigate"
  },
  {
    "key": "ctrl+up",
    "command": "search.action.focusPreviousSearchResult",
    "when": "hasSearchResult || inSearchEditor"
  },
  {
    "key": "ctrl+x ctrl+f",
    "command": "workbench.action.quickOpen"
  },
  {
    "key": "ctrl+x b",
    "command": "bracketLens.toggleMuteAll",
    "when": "editorFocus"
  },
  {
    "key": "ctrl+x d",
    "command": "extension.dired.open",
    "when": "editorTextFocus && !inDebugRepl"
  },
  {
    "key": "ctrl+x e",
    "command": "workbench.action.tasks.runTask",
    "args": "emacs"
  },
  {
    "key": "ctrl+x f",
    "command": "-extension.dired.open",
    "when": "editorTextFocus && !inDebugRepl"
  },
  {
    "key": "ctrl+x g",
    "command": "workbench.action.tasks.runTask",
    "args": "emacs-magit-status"
  },
  {
    "key": "ctrl+x i",
    "command": "settings.cycle.indentGuides",
    "when": "editorFocus"
  },
  {
    "key": "ctrl+x l",
    "command": "settings.cycle.lineNumbers",
    "when": "editorFocus"
  },
  {
    "key": "ctrl+x p",
    "command": "settings.cycle.copilot",
    "when": "editorFocus"
  },
  {
    "key": "ctrl+x r",
    "command": "settings.cycle.rust-analyzer-inlay-hints",
    "when": "editorFocus"
  },
  {
    "key": "ctrl+x shift+ctrl+D",
    "command": "workbench.action.debug.stop",
    "when": "inDebugMode && !focusedSessionIsAttach"
  },
  {
    "key": "ctrl+x shift+z",
    "command": "settings.cycle.zen",
    "when": "editorFocus"
  },
  {
    "key": "ctrl+x z",
    "command": "-workbench.action.toggleZenMode",
    "when": "!terminalFocus"
  },
  {
    "key": "ctrl+x z",
    "command": "workbench.action.toggleZenMode"
  },
  {
    "key": "enter",
    "command": "-renameFile",
    "when": "explorerViewletVisible && filesExplorerFocus && !explorerResourceIsRoot && !explorerResourceReadonly && !inputFocus"
  },
  {
    "key": "enter",
    "command": "extension.multiCommand.execute",
    "args": {
      "sequence": ["list.select", "workbench.action.keepEditor"]
    },
    "when": "listFocus"
  },
  {
    "key": "f12",
    "command": "settings.cycle.zen",
    "when": "editorFocus"
  },
  {
    "key": "shift+cmd+[",
    "command": "workbench.action.terminal.focusPrevious",
    "when": "terminalFocus && terminalProcessSupported"
  },
  {
    "key": "shift+cmd+]",
    "command": "workbench.action.terminal.focusNext",
    "when": "terminalFocus && terminalProcessSupported"
  },
  {
    "key": "shift+enter",
    "command": "renameFile",
    "when": "explorerViewletVisible && filesExplorerFocus && !explorerResourceIsRoot && !explorerResourceReadonly && !inputFocus"
  },
  {
    "key": "shift+tab",
    "command": "editor.foldAll",
    "when": "editorTextFocus && foldingEnabled"
  },
  {
    "key": "cmd+k cmd+0",
    "command": "-editor.foldAll",
    "when": "editorTextFocus && foldingEnabled"
  },
  {
    "key": "cmd+u",
    "command": "editor.unfoldAll",
    "when": "editorTextFocus && foldingEnabled"
  },
  {
    "key": "cmd+k cmd+j",
    "command": "-editor.unfoldAll",
    "when": "editorTextFocus && foldingEnabled"
  },
  {
    "key": "alt+j",
    "command": "workbench.action.navigateBack"
  },
  {
    "key": "ctrl+-",
    "command": "-workbench.action.navigateBack"
  },
  {
    "key": "ctrl+right",
    "command": "editor.action.jumpToBracket",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+left",
    "command": "editor.action.jumpToBracket",
    "when": "editorTextFocus"
  },
  {
    "key": "cmd+u",
    "command": "workbench.action.files.revert"
  }
]
