  {
    "key": "ctrl+k",
    "command": "emacs-mcx.paredit.killSexp",
    "when": "editorTextFocus && !config.emacs-mcx.useMetaPrefixMacCmd"
  },
  {
    "key": "ctrl+alt+k",
    "command": "-emacs-mcx.paredit.killSexp",
    "when": "editorTextFocus && !config.emacs-mcx.useMetaPrefixMacCmd"
  },
