{
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter",
    "editor.formatOnPaste": false,
    "editor.formatOnSaveMode": "file",
    "editor.acceptSuggestionOnCommitCharacter": false,
    "editor.formatOnType": true
  },
  "[sql]": {
    "editor.defaultFormatter": "adpyke.vscode-sql-formatter"
  },
  "[typescript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features",
    "editor.acceptSuggestionOnCommitCharacter": false
  },
  "alt-editor.args": "--line {line} --column {column} {filename}",
  "alt-editor.binary": "/Applications/IntelliJ IDEA.app/Contents/MacOS/idea",
  "diffEditor.renderSideBySide": true,
  "editor.bracketPairColorization.enabled": true,
  "editor.cursorBlinking": "solid",
  "editor.foldingHighlight": false,
  "editor.gotoLocation.multipleDeclarations": "gotoAndPeek",
  "editor.gotoLocation.multipleDefinitions": "gotoAndPeek",
  "editor.gotoLocation.multipleReferences": "gotoAndPeek",
  "editor.gotoLocation.multipleTypeDefinitions": "gotoAndPeek",
  "editor.inlineSuggest.enabled": true,
  "editor.lightbulb.enabled": false,
  "editor.occurrencesHighlight": false,
  "editor.overviewRulerBorder": false,
  "editor.parameterHints.enabled": false,
  "editor.quickSuggestions": {
    "comments": false,
    "other": true,
    "strings": true
  },
  "editor.renderWhitespace": "none",
  "editor.rulers": [],
  "editor.scrollbar.verticalScrollbarSize": 0,
  "editor.suggest.localityBonus": true,
  "editor.suggest.showTypeParameters": true,
  "editor.suggestSelection": "first",
  "editor.wordBasedSuggestions": false,
  "editor.wordSeparators": "_`~!@#$%^&*()-=+[{]}\\|;:'\",.<>/?",
  "emacs-mcx.strictEmacsMove": true,
  "emmet.includeLanguages": {
    "django-html": "html"
  },
  "eslint.format.enable": true,
  "eslint.lintTask.enable": true,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "files.associations": {
    "**/*.aurora": "python",
    "**/*.gitconfig": "toml",
    "**/*.strato": "scala",
    "**/*.html": "html",
    "**/requirements{/**,*}.{txt,in}": "pip-requirements",
    "**/templates/**/*": "django-txt",
    "**/templates/**/*.html": "django-html",
    // "BUILD": "python",
    // "BUILD.bazel": "python",
    "config": "toml"
  },
  "files.exclude": {
    "*.egg-info": true,
    "**/__pycache__": true,
    "**/.*": true,
    "**/.*_cache": true,
    "**/.classpath": true,
    "**/.DS_Store": true,
    "**/.factorypath": true,
    "**/.git": true,
    "**/.hg": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.svn": true,
    "**/*.class": true,
    "**/*.olean": true,
    "**/CVS": true,
    "**/staticfiles": true,
    "**/Thumbs.db": true,
    "docs/_book": true,
    "out": false
  },
  "files.watcherExclude": {
    "**/.ammonite": true,
    "**/.bloop": true,
    "**/.metals": true
  },
  "github.copilot.enable": {
    "*": true
  },
  "github.copilot.inlineSuggest.enable": false,
  "gitlens.codeLens.enabled": false,
  "gitlens.codeLens.recentChange.enabled": false,
  "gitlens.currentLine.enabled": false,
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.hovers.enabled": false,
  "java.project.importOnFirstTimeStartup": "automatic",
  "magit.hide-status-sections": [
    "issues",
    "pull requests",
    "untracked",
    "recent commits",
    "unpulled",
    "unpushed",
    "unmerged",
    "stashes"
  ],
  "magit.quick-switch-enabled": true,
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "prettier.documentSelectors": ["*.ts"],
  "prettier.singleQuote": true,
  "python.analysis.typeCheckingMode": "basic",
  "python.envFile": "${workspaceFolder}/.env.dev",
  "python.formatting.blackArgs": ["--fast", "--line-length", "99"],
  "python.formatting.provider": "none",
  "python.languageServer": "Pylance",
  "python.linting.enabled": true,
  "python.linting.mypyArgs": ["--check-untyped-defs"],
  "python.linting.mypyPath": "/Users/dan/src/owldock/backend/.venv/bin/mypy",
  "python.linting.pylintArgs": ["--load-plugins", "--rcfile=.pylintrc"],
  "python.linting.pylintEnabled": false,
  "python.testing.autoTestDiscoverOnSaveEnabled": false,
  "python.testing.promptToConfigure": false,
  "python.testing.pytestEnabled": true,
  "python.testing.unittestEnabled": false,
  "rewrap.wrappingColumn": 80,
  // "rust-analyzer.checkOnSave.command": "clippy",
  // "rust-analyzer.checkOnSave.extraArgs": ["--", "-D", "warnings"],
  // "rust-analyzer.inlayHints.maxLength": null,
  "scm.diffDecorationsGutterVisibility": "hover",
  "search.showLineNumbers": true,
  "security.workspace.trust.banner": "always",
  "security.workspace.trust.startupPrompt": "always",
  "security.workspace.trust.untrustedFiles": "open",
  "settings.cycle": [
    {
      "id": "formatOnSave",
      "overrideWorkspaceSettings": true,
      "values": [
        {
          "editor.formatOnSave": true
        },
        {
          "editor.formatOnSave": false
        }
      ]
    },
    {
      "id": "indentGuides",
      "overrideWorkspaceSettings": true,
      "values": [
        {
          "editor.guides.indentation": true
        },
        {
          "editor.guides.indentation": false
        }
      ]
    },
    {
      "id": "lineNumbers",
      "overrideWorkspaceSettings": true,
      "values": [
        {
          "editor.lineNumbers": "on"
        },
        {
          "editor.lineNumbers": "off"
        }
      ]
    },
    {
      "id": "matchParens",
      "overrideWorkspaceSettings": true,
      "values": [
        {
          "editor.matchBrackets": "never"
        },
        {
          "editor.matchBrackets": "always"
        }
      ]
    },
    {
      "id": "copilot",
      "overrideWorkspaceSettings": true,
      "values": [
        {
          "github.copilot.inlineSuggest.enable": false
        },
        {
          "github.copilot.inlineSuggest.enable": true
        }
      ]
    },
    {
      "id": "rust-analyzer-inlay-hints",
      "overrideWorkspaceSettings": true,
      "values": [
        {
          "rust-analyzer.inlayHints.enable": false
        },
        {
          "rust-analyzer.inlayHints.enable": true
        }
      ]
    },
    {
      "id": "zen",
      "overrideWorkspaceSettings": true,
      "values": [
        {
          "dan.ZenPlus": false,
          "errorLens.enabled": true,
          "rust-analyzer.inlayHints.enable": true,
          "rust-analyzer.hover.actions.enable": true,
          "editor.matchBrackets": "always",
          "workbench.colorCustomizations": {
            "editorError.foreground": "red",
            "editorInfo.foreground": "blue",
            "editorWarning.foreground": "orange"
          }
        },
        {
          "dan.ZenPlus": true,
          "errorLens.enabled": false,
          "rust-analyzer.inlayHints.enable": false,
          "rust-analyzer.hover.actions.enable": false,
          "editor.matchBrackets": "never",
          "bracketLens.mode": "none",
          "workbench.colorCustomizations": {
            "editorError.foreground": "#00000000",
            "editorInfo.foreground": "#00000000",
            "editorWarning.foreground": "#00000000"
          }
        }
      ]
    }
  ],
  "settings.cycle.logLevel": "log",
  "settingsSync.ignoredExtensions": ["esbenp.prettier-vscode"],
  "statusbar_command.commands": [
    {
      "alignment": "right",
      "id": "document-offset",
      "script": "statusBarItem.text = vscode.window.activeTextEditor.document.offsetAt(vscode.window.activeTextEditor.selection.active)",
      "scriptEvents": [
        "vscode.workspace.onDidSaveTextDocument",
        "vscode.window.onDidChangeActiveTextEditor"
      ],
      "text": "Document offset"
    },
    {
      "alignment": "right",
      "command": "workbench.action.openWorkspaceSettings",
      "id": "sbc_settings",
      "name": "settings",
      "priority": -1000,
      "text": "$(gear)",
      "tooltip": "workspace settings"
    },
    {
      "alignment": "right",
      "command": "workbench.action.terminal.toggleTerminal",
      "id": "sbc_terminal",
      "name": "Terminal",
      "priority": 1000,
      "text": "$(terminal)",
      "tooltip": "Terminal"
    },
    // {
    //   "alignment": "left",
    //   "scriptFile": "/Users/dan/src/devenv/dotfiles/vscode/statusbar_commands/bsp-status.js",
    //   "scriptEvents": [
