    //   ],
    //   "id": "sbc.bsp-status",
    //   "name": "BSP status",
    //   "text": "<BSP status>"
    // },
    {
      "alignment": "right",
      "id": "formatOnSave",
      "script": "statusBarItem.text = vscode.workspace.getConfiguration('editor').formatOnSave ? 'âš¡': ''",
      "scriptEvents": ["vscode.workspace.onDidChangeConfiguration"],
      "text": "Format on save"
    },
    {
      "alignment": "right",
      "command": "github.copilot.toggleCopilot",
      "id": "sbc.copilot",
      "script": "statusBarItem.text = vscode.workspace.getConfiguration('github.copilot.inlineSuggest').enable ? '$(github)' : ''",
      "scriptEvents": ["vscode.workspace.onDidChangeConfiguration"],
      "text": "Github Copilot"
    },
    {
      "alignment": "right",
      "id": "sbc.rust-analyzer-inlay-hints",
      "script": "statusBarItem.text = vscode.workspace.getConfiguration('rust-analyzer.inlayHints').enable ? '$(extensions-info-message)' : ''",
      "scriptEvents": ["vscode.workspace.onDidChangeConfiguration"],
      "text": "rust-analyzer inlay hints"
    },
    {
      "alignment": "right",
      "command": "dan.zen",
      "id": "sbc.zen",
      "script": "statusBarItem.text = vscode.workspace.getConfiguration('errorLens').enabled ? '' : '$(circle-slash)'",
      "scriptEvents": ["vscode.workspace.onDidChangeConfiguration"],
      "text": "Zen Squiggles"
    }
  ],
