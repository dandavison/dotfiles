  "rust-analyzer.inlayHints.maxLength": null,
  "scm.diffDecorationsGutterVisibility": "hover",
  "search.showLineNumbers": true,
  "security.workspace.trust.banner": "always",
  "security.workspace.trust.startupPrompt": "always",
  "security.workspace.trust.untrustedFiles": "open",
  "settings.cycle": [
    {
      "id": "formatOnSave",
      "overrideWorkspaceSettings": true,
      "values": [
        {
          "editor.formatOnSave": true
        },
        {
          "editor.formatOnSave": false
        }
      ]
    },
    {
      "id": "indentGuides",
      "overrideWorkspaceSettings": true,
      "values": [
        {
          "editor.guides.indentation": true
        },
        {
          "editor.guides.indentation": false
        }
      ]
    },
    {
      "id": "lineNumbers",
      "overrideWorkspaceSettings": true,
      "values": [
        {
          "editor.lineNumbers": "on"
        },
        {
          "editor.lineNumbers": "off"
        }
      ]
    },
    {
      "id": "matchParens",
      "overrideWorkspaceSettings": true,
      "values": [
        {
          "editor.matchBrackets": false
        },
        {
          "editor.matchBrackets": true
        }
      ]
    },
    {
      "id": "copilot",
      "overrideWorkspaceSettings": true,
      "values": [
        {
          "github.copilot.inlineSuggest.enable": false
        },
        {
          "github.copilot.inlineSuggest.enable": true
        }
      ]
    },
    {
      "id": "rust-analyzer-inlay-hints",
      "overrideWorkspaceSettings": true,
      "values": [
        {
          "rust-analyzer.inlayHints.enable": false
        },
        {
          "rust-analyzer.inlayHints.enable": true
        }
      ]
    },
    {
      "id": "zen",
      "overrideWorkspaceSettings": true,
      "values": [
        {
          "dan.ZenPlus": true,
          "errorLens.enabled": false,
          "editor.matchBrackets": "never",
          "workbench.colorCustomizations": {
            "editorError.foreground": "#00000000",
            "editorInfo.foreground": "#00000000",
            "editorWarning.foreground": "#00000000"
          }
        },
        {
          "dan.ZenPlus": false,
          "errorLens.enabled": true,
          "editor.matchBrackets": "always",
          "workbench.colorCustomizations": {
            "editorError.foreground": "red",
            "editorInfo.foreground": "blue",
            "editorWarning.foreground": "orange"
          }
        }
      ]
    }
  ],
  "settings.cycle.logLevel": "log",
  "settingsSync.ignoredExtensions": ["esbenp.prettier-vscode"],
  "statusbar_command.commands": [
    {
      "alignment": "right",
      "command": "workbench.action.openWorkspaceSettings",
      "id": "sbc_settings",
      "name": "settings",
      "priority": -1000,
      "text": "$(gear)",
      "tooltip": "workspace settings"
    },
    {
      "alignment": "right",
      "command": "workbench.action.terminal.toggleTerminal",
      "id": "sbc_terminal",
      "name": "Terminal",
      "priority": 1000,
      "text": "$(terminal)",
      "tooltip": "Terminal"
    },
    {
      "alignment": "left",
      "scriptFile": "/Users/ddavison/src/devenv/dotfiles/vscode/statusbar_commands/bsp-status.js",
      "scriptEvents": [
        "vscode.workspace.onDidSaveTextDocument",
        "vscode.window.onDidChangeActiveTextEditor",
        "vscode.workspace.onDidChangeConfiguration"
      ],
      "id": "sbc.bsp-status",
      "name": "BSP status",
      "text": "<BSP status>"
    },
    {
      "alignment": "right",
      "id": "formatOnSave",
      "script": "statusBarItem.text = vscode.workspace.getConfiguration('editor').formatOnSave ? 'âš¡': ''",
      "scriptEvents": ["vscode.workspace.onDidChangeConfiguration"],
      "text": "Format on save"
    },
    {
      "alignment": "right",
      "command": "github.copilot.toggleCopilot",
      "id": "sbc.copilot",
      "script": "statusBarItem.text = vscode.workspace.getConfiguration('github.copilot.inlineSuggest').enable ? '$(github)' : ''",
      "scriptEvents": ["vscode.workspace.onDidChangeConfiguration"],
      "text": "Github Copilot"
    },
    {
      "alignment": "right",
      "id": "sbc.rust-analyzer-inlay-hints",
      "script": "statusBarItem.text = vscode.workspace.getConfiguration('rust-analyzer.inlayHints').enable ? '$(extensions-info-message)' : ''",
      "scriptEvents": ["vscode.workspace.onDidChangeConfiguration"],
      "text": "rust-analyzer inlay hints"
    },
    {
      "alignment": "right",
      "command": "dan.zen",
      "id": "sbc.zen",
      "script": "statusBarItem.text = vscode.workspace.getConfiguration('editor').matchBrackets === 'never' ? '$(circle-slash)' : ''",
      "scriptEvents": ["vscode.workspace.onDidChangeConfiguration"],
      "text": "Zen Squiggles"
    }
  ],
