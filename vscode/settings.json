    "editor.formatOnSaveMode": "file"
  },
  "[sql]": {
    "editor.defaultFormatter": "adpyke.vscode-sql-formatter"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[vue]": {
    "editor.defaultFormatter": "octref.vetur"
  },
  "alt-editor.args": "--line {line} --column {column} {filename}",
  "alt-editor.binary": "/Applications/IntelliJ IDEA.app/Contents/MacOS/idea",
  "diffEditor.renderSideBySide": true,
  "editor.bracketPairColorization.enabled": true,
  "editor.cursorBlinking": "solid",
  "editor.foldingHighlight": false,
  "editor.gotoLocation.multipleDeclarations": "gotoAndPeek",
  "editor.gotoLocation.multipleDefinitions": "gotoAndPeek",
  "editor.gotoLocation.multipleReferences": "gotoAndPeek",
  "editor.gotoLocation.multipleTypeDefinitions": "gotoAndPeek",
  "editor.inlineSuggest.enabled": true,
  "editor.lightbulb.enabled": false,
  "editor.lineNumbers": "off",
  "editor.matchBrackets": "always",
  "editor.minimap.enabled": false,
  "editor.occurrencesHighlight": false,
  "editor.overviewRulerBorder": false,
  "editor.parameterHints.enabled": false,
  "editor.quickSuggestions": {
    "comments": false,
    "other": true,
    "strings": false
  },
  "editor.renderLineHighlight": "none",
  "editor.renderWhitespace": "none",
  "editor.rulers": [],
  "editor.scrollbar.verticalScrollbarSize": 0,
  "editor.suggest.localityBonus": true,
  "editor.suggest.showTypeParameters": true,
  "editor.suggestSelection": "first",
  "editor.wordBasedSuggestions": false,
  "editor.wordSeparators": "_`~!@#$%^&*()-=+[{]}\\|;:'\",.<>/?",
  "emacs-mcx.strictEmacsMove": true,
  "emmet.includeLanguages": {
    "django-html": "html"
  },
  "eslint.format.enable": true,
  "eslint.lintTask.enable": true,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "files.associations": {
    "**/*.aurora": "python",
    "**/*.gitconfig": "toml",
    "**/*.strato": "scala",
    "**/*.html": "html",
    "**/requirements{/**,*}.{txt,in}": "pip-requirements",
    "**/templates/**/*": "django-txt",
    "**/templates/**/*.html": "django-html",
    // "BUILD": "python",
    // "BUILD.bazel": "python",
    "config": "toml"
  },
  "files.exclude": {
    "**/*.class": true,
    "**/*.olean": true,
    "**/.*": true,
    "**/.*_cache": true,
    "**/.DS_Store": true,
    "**/.classpath": true,
    "**/.factorypath": true,
    "**/.git": true,
    "**/.hg": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.svn": true,
    "**/CVS": true,
    "**/Thumbs.db": true,
    "**/__pycache__": true,
    "**/staticfiles": true,
    "docs/_book": true,
    "out": false
  },
  "files.watcherExclude": {
    "**/.ammonite": true,
    "**/.bloop": true,
    "**/.metals": true
  },
  "github.copilot.enable": {
    "*": true
  },
  "github.copilot.inlineSuggest.enable": false,
  "gitlens.codeLens.enabled": false,
  "gitlens.codeLens.recentChange.enabled": false,
  "gitlens.currentLine.enabled": false,
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.hovers.enabled": false,
  "java.project.importOnFirstTimeStartup": "automatic",
  "magit.hide-status-sections": [
    "issues",
    "pull requests",
    "untracked",
    "recent commits",
    "unpulled",
    "unpushed",
    "unmerged",
    "stashes"
  ],
  "magit.quick-switch-enabled": true,
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "prettier.documentSelectors": ["*.ts"],
  "prettier.singleQuote": true,
  "python.analysis.typeCheckingMode": "basic",
  "python.envFile": "${workspaceFolder}/.env.dev",
  "python.formatting.blackArgs": ["--fast", "--line-length", "99"],
  "python.formatting.provider": "black",
  "python.languageServer": "Pylance",
  "python.linting.enabled": true,
  "python.linting.mypyArgs": ["--check-untyped-defs"],
  "python.linting.mypyPath": "/Users/dan/src/owldock/backend/.venv/bin/mypy",
  "python.linting.pylintArgs": ["--load-plugins", "--rcfile=.pylintrc"],
  "python.linting.pylintEnabled": false,
