{
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[python]": {
    "editor.formatOnPaste": false,
    "editor.formatOnSaveMode": "file"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[vue]": {
    "editor.defaultFormatter": "octref.vetur"
  },
  "atlascode.bitbucket.enabled": false,
  "atlascode.jira.enabled": true,
  "atlascode.jira.jqlList": [
    {
      "enabled": true,
      "id": "6d23e0f3-a0af-41f1-b8f5-1ead4fdb5f1f",
      "monitor": true,
      "name": "My mygn Issues",
      "query": "assignee = currentUser() AND resolution = Unresolved NOT status = DONE ORDER BY lastViewed DESC",
      "siteId": "aa0e015a-6c18-44d1-806f-12425f81877c"
    }
  ],
  "bracket-pair-colorizer-2.colors": ["DarkGreen", "DarkRed", "DarkBlue"],
  "breadcrumbs.enabled": true,
  "diffEditor.renderSideBySide": true,
  "editor.cursorBlinking": "solid",
  "editor.foldingHighlight": false,
  "editor.lightbulb.enabled": false,
  "editor.matchBrackets": "always",
  "editor.minimap.enabled": false,
  "editor.occurrencesHighlight": false,
  "editor.parameterHints.enabled": false,
  "editor.quickSuggestions": true,
  "editor.renderLineHighlight": "none",
  "editor.renderWhitespace": "none",
  "editor.rulers": [],
  "editor.scrollbar.verticalScrollbarSize": 0,
  "editor.suggest.localityBonus": true,
  "editor.suggest.showTypeParameters": true,
  "editor.suggestSelection": "first",
  "editor.tabCompletion": "on",
  "emacs-mcx.strictEmacsMove": true,
  "emmet.includeLanguages": {
    "django-html": "html"
  },
  "files.associations": {
    "**/*.html": "html",
    "**/requirements{/**,*}.{txt,in}": "pip-requirements",
    "**/templates/**/*": "django-txt",
    "**/templates/**/*.html": "django-html"
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/__pycache__": true,
    "**/.*": true,
    "**/.*_cache": true,
    "**/*.olean": true,
    "docs/_book": true,
    "out": false
  },
  "gitlens.codeLens.enabled": false,
  "gitlens.codeLens.recentChange.enabled": false,
  "gitlens.currentLine.enabled": false,
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.hovers.enabled": false,
  "magit.quick-switch-enabled": true,
  "magit.hide-status-sections": [
    "recent commits",
    "unpulled",
    "unpushed",
    "unmerged",
    "stashes"
  ],
  "prettier.documentSelectors": ["*.ts"],
  "prettier.proseWrap": "always",
  "python.analysis.typeCheckingMode": "basic",
  "python.autoComplete.showAdvancedMembers": false,
  "python.formatting.provider": "black",
  "python.languageServer": "Microsoft",
  "python.linting.mypyArgs": [
    "--ignore-missing-imports",
    "--follow-imports=silent",
    "--check-untyped-defs"
  ],
  "python.linting.mypyEnabled": true,
  "python.linting.pylintArgs": ["--load-plugins", "--rcfile=.pylintrc"],
  "python.linting.pylintEnabled": false,
  "python.linting.pylintUseMinimalCheckers": false,
  "rust-analyzer.inlayHints.enable": false,
