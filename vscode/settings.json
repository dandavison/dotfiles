    {
      "id": "lineNumbers",
      "overrideWorkspaceSettings": true,
      "values": [
        {
          "editor.lineNumbers": true
        },
        {
          "editor.lineNumbers": false
        }
      ]
    },
    {
      "id": "zen",
      "overrideWorkspaceSettings": true,
      "values": [
        {
          "editor.matchBrackets": "never",
          "workbench.colorCustomizations": {
            "editorError.foreground": "#00000000",
            "editorWarning.foreground": "#00000000",
            "editorInfo.foreground": "#00000000"
          }
        },
        {
          "editor.matchBrackets": "always",
          "workbench.colorCustomizations": {
            "editorError.foreground": "red",
            "editorWarning.foreground": "orange",
            "editorInfo.foreground": "blue"
          }
        }
      ]
    }
  ],
  "settingsSync.ignoredExtensions": ["esbenp.prettier-vscode"],
  "terminal.external.osxExec": "iTerm 2.app",
  "terminal.integrated.rendererType": "experimentalWebgl",
  "todo-tree.general.tags": [
    "BUG",
    "HACK",
    "FIXME",
    "TODO",
    "XXX",
    "[ ]",
    "[x]"
  ],
  "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
  "todo-tree.tree.showScanModeButton": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "vetur.validation.interpolation": false,
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "window.nativeFullScreen": false,
  "window.zoomLevel": 1,
  "workbench.activityBar.visible": false,
  "workbench.editorAssociations": [
    {
      "filenamePattern": "*.ipynb",
      "viewType": "jupyter.notebook.ipynb"
    }
  ],
  "workbench.sideBar.location": "right",
  "yaml.schemas": {
    "file:///Users/dan/.vscode/extensions/atlassian.atlascode-2.8.6/resources/schemas/pipelines-schema.json": "bitbucket-pipelines.yml"
