{
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.python",
    "editor.formatOnPaste": false,
    "editor.formatOnSaveMode": "file"
  },
  "[sql]": {
    "editor.defaultFormatter": "adpyke.vscode-sql-formatter"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[vue]": {
    "editor.defaultFormatter": "octref.vetur"
  },
  "alt-editor.args": "--line {line} --column {column} {filename}",
  "alt-editor.binary": "/Applications/IntelliJ IDEA.app/Contents/MacOS/idea",
  "breadcrumbs.symbolPath": "off",
  "diffEditor.renderSideBySide": true,
  "editor.bracketPairColorization.enabled": true,
  "editor.cursorBlinking": "solid",
  "editor.foldingHighlight": false,
  "editor.formatOnSave": true,
  "editor.gotoLocation.multipleDeclarations": "gotoAndPeek",
  "editor.gotoLocation.multipleDefinitions": "gotoAndPeek",
  "editor.gotoLocation.multipleReferences": "gotoAndPeek",
  "editor.gotoLocation.multipleTypeDefinitions": "gotoAndPeek",
  "editor.guides.indentation": false,
  "editor.inlineSuggest.enabled": true,
  "editor.lightbulb.enabled": false,
  "editor.lineNumbers": "off",
  "editor.matchBrackets": "always",
  "editor.minimap.enabled": false,
  "editor.occurrencesHighlight": false,
  "editor.overviewRulerBorder": false,
  "editor.parameterHints.enabled": false,
  "editor.quickSuggestions": {
    "comments": false,
    "other": true,
    "strings": false
  },
  "editor.renderLineHighlight": "none",
  "editor.renderWhitespace": "none",
  "editor.rulers": [],
  "editor.scrollbar.verticalScrollbarSize": 0,
  "editor.suggest.localityBonus": true,
  "editor.suggest.showTypeParameters": true,
  "editor.suggestSelection": "first",
  "editor.wordBasedSuggestions": false,
  "editor.wordSeparators": "_`~!@#$%^&*()-=+[{]}\\|;:'\",.<>/?",
  "emacs-mcx.strictEmacsMove": true,
  "emmet.includeLanguages": {
    "django-html": "html"
  },
  "eslint.format.enable": true,
  "eslint.lintTask.enable": true,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "files.associations": {
    "**/*.html": "html",
    "**/*.strato": "scala",
    "**/requirements{/**,*}.{txt,in}": "pip-requirements",
    "**/templates/**/*": "django-txt",
    "**/templates/**/*.html": "django-html"
  },
  "files.exclude": {
    "**/*.class": true,
    "**/*.olean": true,
    "**/.*": true,
    "**/.*_cache": true,
    "**/.DS_Store": true,
    "**/.classpath": true,
    "**/.factorypath": true,
    "**/.git": true,
    "**/.hg": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.svn": true,
    "**/CVS": true,
    "**/Thumbs.db": true,
    "**/__pycache__": true,
    "**/staticfiles": true,
    "docs/_book": true,
    "out": false
  },
  "files.watcherExclude": {
    "**/.ammonite": true,
    "**/.bloop": true,
    "**/.metals": true
  },
  "github.copilot.enable": {
    "*": true
  },
  "github.copilot.inlineSuggest.enable": true,
  "gitlens.codeLens.enabled": false,
  "gitlens.codeLens.recentChange.enabled": false,
  "gitlens.currentLine.enabled": false,
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.hovers.enabled": false,
  "java.project.importOnFirstTimeStartup": "automatic",
  "magit.hide-status-sections": [
    "recent commits",
    "unpulled",
    "unpushed",
    "unmerged",
    "stashes"
  ],
  "magit.quick-switch-enabled": true,
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "prettier.documentSelectors": ["*.ts"],
  "prettier.singleQuote": true,
  "pylance.insidersChannel": "daily",
  "python.analysis.typeCheckingMode": "basic",
  "python.envFile": "${workspaceFolder}/.env.dev",
  "python.formatting.blackArgs": ["--fast"],
  "python.formatting.provider": "black",
  "python.languageServer": "Pylance",
  "python.linting.enabled": true,
  "python.linting.flake8Enabled": true,
  "python.linting.mypyArgs": ["--check-untyped-defs"],
  "python.linting.mypyPath": "/Users/dan/src/owldock/backend/.venv/bin/mypy",
  "python.linting.pylintArgs": ["--load-plugins", "--rcfile=.pylintrc"],
  "python.linting.pylintEnabled": false,
  "python.testing.autoTestDiscoverOnSaveEnabled": false,
  "python.testing.promptToConfigure": false,
  "python.testing.pytestEnabled": true,
  "python.testing.unittestEnabled": false,
  "redhat.telemetry.enabled": false,
  "rewrap.wrappingColumn": 100,
  "rust-analyzer.checkOnSave.command": "clippy",
  "rust-analyzer.checkOnSave.extraArgs": ["--", "-D", "warnings"],
  "rust-analyzer.inlayHints.enable": false,
  "rust-analyzer.inlayHints.maxLength": null,
  "rust-client.updateOnStartup": true,
  "saveEditorLayout.list": [
    {
      "documents": [
        {
          "column": 1,
          "fsPath": "/Users/dan/src/owldock/ui/src/factories.ts"
        },
        {
          "column": 1,
          "fsPath": "/Users/dan/src/owldock/ui/src/components/CaseForm.vue"
        },
        {
          "column": 1,
          "fsPath": "/Users/dan/src/owldock/ui/src/components/ProviderContactSelector.vue"
        },
        {
          "column": 1,
          "fsPath": "/Users/dan/src/owldock/ui/src/autogenerated-interfaces.ts"
        },
        {
          "column": 3,
          "fsPath": "/Users/dan/src/owldock/ui/src/components/CaseStep.vue"
        }
      ],
      "name": "case-applicant-step",
      "orientation": 0
    }
  ],
  "scm.diffDecorationsGutterVisibility": "hover",
  "search.showLineNumbers": false,
  "security.workspace.trust.banner": "never",
  "security.workspace.trust.startupPrompt": "never",
  "security.workspace.trust.untrustedFiles": "open",
  "settings.cycle": [
    {
      "id": "indentGuides",
      "overrideWorkspaceSettings": true,
      "values": [
        {
          "editor.renderIndentGuides": true
        },
        {
          "editor.renderIndentGuides": false
        }
      ]
    },
    {
      "id": "lineNumbers",
      "overrideWorkspaceSettings": true,
      "values": [
        {
          "editor.lineNumbers": "on"
        },
        {
          "editor.lineNumbers": "off"
        }
      ]
    },
    {
      "id": "matchParens",
      "overrideWorkspaceSettings": true,
      "values": [
        {
          "editor.matchBrackets": false
        },
        {
          "editor.matchBrackets": true
        }
      ]
    },
    {
      "id": "copilot",
      "overrideWorkspaceSettings": true,
      "values": [
        {
          "github.copilot.inlineSuggest.enable": false
        },
        {
          "github.copilot.inlineSuggest.enable": true
        }
      ]
    },
    {
      "id": "rust-analyzer-inlay-hints",
      "overrideWorkspaceSettings": true,
      "values": [
        {
          "rust-analyzer.inlayHints.enable": false
        },
        {
          "rust-analyzer.inlayHints.enable": true
        }
      ]
    },
    {
      "id": "zen",
      "overrideWorkspaceSettings": true,
      "values": [
        {
          "dan.ZenPlus": true,
          "editor.matchBrackets": "never",
          "workbench.colorCustomizations": {
            "editorError.foreground": "#00000000",
            "editorInfo.foreground": "#00000000",
            "editorWarning.foreground": "#00000000"
          }
        },
        {
          "dan.ZenPlus": false,
          "editor.matchBrackets": "always",
          "workbench.colorCustomizations": {
            "editorError.foreground": "red",
            "editorInfo.foreground": "blue",
            "editorWarning.foreground": "orange"
          }
        }
      ]
    }
  ],
  "settings.cycle.logLevel": "log",
  "settingsSync.ignoredExtensions": ["esbenp.prettier-vscode"],
  "sourcegraph.url": "https://sourcegraph.twitter.biz/",
  "statusbar_command.commands": [
    {
      "alignment": "right",
      "command": "workbench.action.openWorkspaceSettings",
      "id": "sbc_settings",
      "name": "settings",
      "priority": -1000,
      "text": "$(gear)",
      "tooltip": "workspace settings"
    },
    {
      "alignment": "right",
      "command": "workbench.action.terminal.toggleTerminal",
      "id": "sbc_terminal",
      "name": "Terminal",
      "priority": 1000,
      "text": "$(terminal)",
      "tooltip": "Terminal"
    },
    {
      "alignment": "right",
      "command": "github.copilot.toggleCopilot",
      "id": "sbc.copilot",
      "script": "statusBarItem.text = vscode.workspace.getConfiguration('github.copilot.inlineSuggest').enable ? '$(github)' : '$(circle-large-outline)'",
      "scriptEvents": ["vscode.workspace.onDidChangeConfiguration"],
      "text": "Github Copilot"
    },
    {
      "alignment": "right",
      "id": "sbc.rust-analyzer-inlay-hints",
      "script": "statusBarItem.text = vscode.workspace.getConfiguration('rust-analyzer.inlayHints').enable ? '$(extensions-info-message)' : '$(circle-large-outline)'",
      "scriptEvents": ["vscode.workspace.onDidChangeConfiguration"],
      "text": "rust-analyzer inlay hints"
    },
    {
      "alignment": "right",
      "command": "dan.zen",
      "id": "sbc.zen",
      "script": "statusBarItem.text = vscode.workspace.getConfiguration('editor').matchBrackets === 'never' ? '$(circle-slash)' : '$(circle-large-outline)'",
      "scriptEvents": ["vscode.workspace.onDidChangeConfiguration"],
      "text": "Zen Squiggles"
    }
  ],
  "task.saveBeforeRun": "never",
  "terminal.explorerKind": "external",
  "terminal.external.osxExec": "iTerm 2.app",
  "terminal.integrated.tabs.enabled": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "vetur.validation.interpolation": false,
  "window.nativeFullScreen": false,
  "window.zoomLevel": 1,
  "workbench.activityBar.visible": false,
  "workbench.colorCustomizations": {
    "editorError.foreground": "red",
    "editorInfo.foreground": "blue",
    "editorWarning.foreground": "orange"
  },
  "workbench.colorTheme": "GitHub Light",
  "workbench.editor.untitled.hint": "hidden",
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "workbench.panel.opensMaximized": "always",
  "workbench.sideBar.location": "right",
  "workbench.startupEditor": "none",
  "workbench.tree.indent": 33,
  "yaml.schemas": {
    "file:///Users/dan/.vscode/extensions/atlassian.atlascode-2.9.1/resources/schemas/pipelines-schema.json": "bitbucket-pipelines.yml"
  },
  "zenMode.centerLayout": false,
  "zenMode.hideLineNumbers": false,
  "zenMode.hideStatusBar": false,
  "zenMode.restore": true
}
