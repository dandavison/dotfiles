  "prettifySymbolsMode.substitutions": [
    {
      // https://github.com/siegebell/vsc-prettify-symbols-mode/wiki/Python
      "language": "python",
      "substitutions": [
        { "ugly": ">=", "pretty": "≥", "pre": "[^=\\-<>]|^", "post": "[^=\\-<>]|$" },
        { "ugly": "<=", "pretty": "≤", "pre": "[^=\\-<>]|^", "post": "[^=\\-<>]|$" },
        { "ugly": "!=", "pretty": "≠", "pre": "[^<>=!]|^", "post": "[^<>=]|$" },
        { "ugly": "lambda", "pretty": "λ", "pre": "\\b", "post": "\\b" },
      ]
    }
  ]
