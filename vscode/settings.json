  "statusbar_command.commands": [
    {
      "alignment": "right",
      "id": "document-offset",
      "script": "statusBarItem.text = vscode.window.activeTextEditor.document.offsetAt(vscode.window.activeTextEditor.selection.active)",
      "scriptEvents": [
        "vscode.workspace.onDidSaveTextDocument",
        "vscode.window.onDidChangeActiveTextEditor"
      ],
      "text": "Document offset"
    },
    {
      "alignment": "right",
      "command": "workbench.action.openWorkspaceSettings",
      "id": "sbc_settings",
      "name": "settings",
      "priority": -1000,
      "text": "$(gear)",
      "tooltip": "workspace settings"
    },
    {
      "alignment": "right",
      "command": "workbench.action.terminal.toggleTerminal",
      "id": "sbc_terminal",
      "name": "Terminal",
      "priority": 1000,
      "text": "$(terminal)",
      "tooltip": "Terminal"
    },
    {
      "alignment": "right",
      "id": "formatOnSave",
      "script": "statusBarItem.text = vscode.workspace.getConfiguration('editor').formatOnSave ? '⚡': ''",
      "scriptEvents": ["vscode.workspace.onDidChangeConfiguration"],
      "text": "Format on save"
    },
    {
      "alignment": "right",
      "id": "sbc.rust-analyzer-inlay-hints",
      "script": "statusBarItem.text = vscode.workspace.getConfiguration('rust-analyzer.inlayHints').enable ? '$(extensions-info-message)' : ''",
      "scriptEvents": ["vscode.workspace.onDidChangeConfiguration"],
      "text": "rust-analyzer inlay hints"
    },
    {
      "alignment": "right",
      "command": "dan.zen",
      "id": "sbc.zen",
      "script": "statusBarItem.text = vscode.workspace.getConfiguration('errorLens').enabled ? '' : '$(circle-slash)'",
      "scriptEvents": ["vscode.workspace.onDidChangeConfiguration"],
      "text": "Zen Squiggles"
    }
  ],
