{
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[python]": {
    "editor.formatOnPaste": false,
    "editor.formatOnSaveMode": "file"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[vue]": {
    "editor.defaultFormatter": "octref.vetur"
  },
  "diffEditor.renderSideBySide": true,
  "editor.cursorBlinking": "solid",
  "editor.formatOnSave": true,
  "editor.lightbulb.enabled": false,
  "editor.minimap.enabled": false,
  "editor.occurrencesHighlight": false,
  "editor.parameterHints.enabled": false,
  "editor.quickSuggestions": true,
  "editor.renderLineHighlight": "none",
  "editor.renderWhitespace": "none",
  "editor.rulers": [],
  "editor.scrollbar.verticalScrollbarSize": 0,
  "editor.snippetSuggestions": "none",
  "editor.suggest.showTypeParameters": true,
  "editor.tabCompletion": "on",
  "emacs-mcx.strictEmacsMove": true,
  "emmet.includeLanguages": {
    "django-html": "html"
  },
  "files.associations": {
    "**/*.html": "html",
    "**/requirements{/**,*}.{txt,in}": "pip-requirements",
    "**/templates/**/*": "django-txt",
    "**/templates/**/*.html": "django-html"
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/*.olean": true,
    "out": false
  },
  "prettier.proseWrap": "always",
  "python.formatting.provider": "black",
  "python.linting.enabled": true,
  "python.linting.mypyArgs": [
    "--ignore-missing-imports",
    "--follow-imports=silent",
    "--check-untyped-defs"
  ],
  "python.linting.mypyEnabled": true,
  "python.linting.pylintEnabled": true,
  "python.linting.pylintUseMinimalCheckers": false,
  "python.linting.pylintArgs": ["--load-plugins"],
  "scm.diffDecorationsGutterVisibility": "hover",
  "window.nativeFullScreen": false,
  "window.zoomLevel": 1,
  "workbench.editorAssociations": [
    {
      "filenamePattern": "*.ipynb",
      "viewType": "jupyter.notebook.ipynb"
    }
  ],
  "zenMode.centerLayout": false,
  "zenMode.hideLineNumbers": false,
  "zenMode.restore": true,
  "python.languageServer": "Pylance",
  "python.testing.autoTestDiscoverOnSaveEnabled": false,
  "gitlens.codeLens.enabled": false,
  "gitlens.codeLens.recentChange.enabled": false,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "zenMode.hideStatusBar": false,
  "rust-analyzer.inlayHints.enable": false,
  "editor.lineNumbers": "off",
  "python.analysis.typeCheckingMode": "basic",
  "breadcrumbs.enabled": false,
  "settingsSync.ignoredExtensions": ["esbenp.prettier-vscode"],
  "prettier.documentSelectors": ["*.ts"],
  "workbench.sideBar.location": "right",
  "terminal.external.osxExec": "iTerm 2.app",
  "workbench.activityBar.visible": false,
  "terminal.integrated.rendererType": "experimentalWebgl",
  "workbench.colorTheme": "Default Light+",
  "debug.toolBarLocation": "hidden",
  "editor.renderIndentGuides": false,
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "todo-tree.tree.showScanModeButton": false,
  "yaml.schemas": {
    "file:///Users/dan/.vscode/extensions/atlassian.atlascode-2.8.6/resources/schemas/pipelines-schema.json": "bitbucket-pipelines.yml"
  },
  "atlascode.jira.enabled": true,
  "atlascode.bitbucket.enabled": false,
  "atlascode.jira.jqlList": [
    {
      "id": "6d23e0f3-a0af-41f1-b8f5-1ead4fdb5f1f",
      "enabled": true,
      "name": "My mygn Issues",
      "query": "assignee = currentUser() AND resolution = Unresolved NOT status = DONE ORDER BY lastViewed DESC",
      "siteId": "aa0e015a-6c18-44d1-806f-12425f81877c",
      "monitor": true
    }
  ],
  "todo-tree.general.tags": [
    "BUG",
    "HACK",
    "FIXME",
    "TODO",
    "XXX",
    "[ ]",
    "[x]"
  ],
  "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
  "bracket-pair-colorizer-2.colors": [
    "DarkGreen",
    "DarkRed",
    "DarkBlue"
  ],
  "editor.matchBrackets": "near"
}
